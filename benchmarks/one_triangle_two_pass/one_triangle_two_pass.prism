mdp

module grid
	x : [0..5000] init 0;
	y : [0..5000] init 0;

	[] 2500*(y - 2500) - 0*(x - 0) >= 0 & -2500*(y - 0) - 2500*(x - 2500) >= 0 & 0*(y - 0) - -2500*(x - 0) >= 0 -> (x' = x + 1);
	

	[] 2500*(y - 2500) - -2500*(x - 0) >= 0 & 0*(y - 2500) - 5000*(x - 2500) >= 0 & -2500*(y - 0) - -2500*(x - 2500) >= 0 -> (x' = x - 1);
	[] 2500*(y - 2500) - -2500*(x - 0) >= 0 & 0*(y - 2500) - 5000*(x - 2500) >= 0 & -2500*(y - 0) - -2500*(x - 2500) >= 0 -> (x' = x + 1);
	

	[] 2500*(y - 2500) - 2500*(x - 0) >= 0 & -2500*(y - 0) - 0*(x - 2500) >= 0 & 0*(y - 0) - -2500*(x - 0) >= 0 -> (x' = x - 1);
	

	[] 0*(y - 2500) - -5000*(x - 2500) >= 0 & 2500*(y - 5000) - 0*(x - 2500) >= 0 & 0*(y - 5000) - 5000*(x - 5000) >= 0 & -2500*(y - 2500) - 0*(x - 5000) >= 0 -> (x' = x - 1);
	[] 0*(y - 2500) - -5000*(x - 2500) >= 0 & 2500*(y - 5000) - 0*(x - 2500) >= 0 & 0*(y - 5000) - 5000*(x - 5000) >= 0 & -2500*(y - 2500) - 0*(x - 5000) >= 0 -> (y' = y + 1);
	

endmodule

label "target" = 2500*(y - 2500) - 2500*(x - 0) >= 0 & -2500*(y - 0) - 0*(x - 2500) >= 0 & 0*(y - 0) - -2500*(x - 0) >= 0;

