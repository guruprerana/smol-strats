mdp

module grid
    x : [0..100] init 1;
    y : [0..100] init 50;

    [R] x > 0 & x < 100 & y > 0 & y < 100 -> 0.8 : (x' = x + 1) + 0.1 : (y' = y + 1) + 0.1 : (y' = y - 1);
    [U] x > 0 & x < 100 & y > 0 & y < 100 -> 0.8 : (y' = y + 1) + 0.1 : (x' = x + 1) + 0.1 : (y' = y - 1);
    [D] x > 0 & x < 100 & y > 0 & y < 100 -> 0.8 : (y' = y - 1) + 0.1 : (x' = x + 1) + 0.1 : (y' = y + 1);

endmodule

label "target" = x <= 100 & x >= 100 & y <= 50 & y >= 50;
